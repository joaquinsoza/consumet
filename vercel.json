{
    "version": 2,
    "installCommand": "npm install && npm run build:extensions",
    "buildCommand": "npm run build",
    "builds": [
      {
        "src": "/src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/main.ts"
      }
    ]
  }
